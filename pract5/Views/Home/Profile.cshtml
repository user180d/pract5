@model UserViewModel

@{
    ViewBag.Title = "Profile";
}

<h2>@ViewData["Title"]</h2>

<div>
    <dl>
        <dt>User name: </dt>
        <dd>@Model.nickname</dd>

        <dt>Email: </dt>
        <dd>@Model.email</dd>

        <dt>Mobile phone: </dt>
        <dd>@Model.phone</dd>

        <dt>About: </dt>
        <dd>@Model.about</dd>
    </dl>
</div>